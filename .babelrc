{
    "presets": ["env", "stage-3", "react", "flow"],
    "plugins": [
        "react-hot-loader/babel",
        "emotion",
        "transform-class-properties", ["module-resolver", {
            "alias": {
                "~": "./app/src",
                "~assets": "./app/public/assets"
            }
        }]
    ]
}
